{
  "kind": "collectionType",
  "collectionName": "comps",
  "info": {
    "singularName": "comp",
    "pluralName": "comps",
    "displayName": "comps",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "maxLength": 50,
      "required": true,
      "minLength": 5
    },
    "comp_string": {
      "type": "text",
      "required": true,
      "regex": "^XQAAAA[A-Za-z0-9_-]+$"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "comps"
    },
    "upvoters": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "private": true,
      "mappedBy": "upvoted_comps"
    },
    "uuid": {
      "type": "uid",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "comps"
    },
    "heroes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::hero.hero",
      "inversedBy": "comps"
    },
    "score": {
      "type": "float",
      "default": 0,
      "min": 0
    },
    "downvoters": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "private": true,
      "inversedBy": "downvoted_comps"
    },
    "downvotes": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "upvotes": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "comp_update": {
      "type": "datetime",
      "required": true
    },
    "saved_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "private": true,
      "inversedBy": "saved_comps"
    },
    "saves": {
      "type": "integer",
      "default": 0,
      "min": 0
    }
  }
}
